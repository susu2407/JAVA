package sub2;
/*
 *  날짜 : 2025/07/21
 *  이름 : 이수연
 *  내용 : 자바 String 클래스 실습하기
 */

public class StringTest {
	
	public static void main(String[] args) {
	// 문자열 이란, 문자 + 배열		
		
	// 문자열 생성
		String str1 = "Bello"; // 문자열 literal // 문자를 쉽게 표기하는 것.
		String str2 = new String("Bello");
		
		System.out.println("str1 : " + str1);
		System.out.println("str2 : " + str2);
		
	// 문자열 비교
		if(str1 == str2) {
			System.out.println("str1, str2 문자열 주소값 같다.");
		}else {
			System.out.println("str1, str2 문자열 주소값 다르다.");			
		} // 출력 ㅣ str1, str2 문자열이 다르다.
		
		if(str1.equals(str2)) {
			System.out.println("str1, str2 문자열이 같다.");
		}else {
			System.out.println("str1, str2 문자열이 다르다.");			
		} // 출력 ㅣ str1, str2 문자열이 같다
		
		
		
	// 문자열 길이
		
		
	// 문자열 추출
		
	// 문자열 자르기
		
	// 문자열 인덱스
		
	// 문자열 분리
		
	// 문자열 교체
		
	// 문자열 변환
		
		
		
		
	}

}
