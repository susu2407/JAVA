package step4;

import java.util.Scanner;

public class Main3 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int count = sc.nextInt();
		
		for (int i =0; i<count; i++) {
			int num = sc.nextInt();
			int [] nums = new int[num];
			
			System.out.println(nums[num]);
			
		}
		
		
	}

}
